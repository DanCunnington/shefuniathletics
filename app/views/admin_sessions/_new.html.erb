
<div class="signup_cont">
	<%= form_for(:admin_session, url: admin_sessions_path) do |f| %>

	<div class="field">
		<%= f.label :email, class: "control-label"%>
		<%= f.text_field :email, id: "email_login",class: "form-control"%>
	</div>
	<div class="field">
		<%= f.label :password,class: "control-label" %>
		<%= f.password_field :password, id: "pass_login" ,class: "form-control"%>
	</div>


	<div style="padding-top:10px;"></div>
	<%= f.submit "Sign in", id: "sign_in_login" ,class: "btn btn-warning btn-medium btn-primary"  %>
	<% end %>
</div>